<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/preloadFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.mobilescanner.main.main.ui.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_captureFragment"
            app:destination="@id/captureFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
        <action
            android:id="@+id/action_mainFragment_to_projectEditFragment"
            app:destination="@id/projectEditFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
        <action
            android:id="@+id/action_mainFragment_to_noteEditFragment"
            app:destination="@id/noteEditFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
        <action
            android:id="@+id/action_mainFragment_to_settingsFragment"
            app:destination="@id/settingsFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
        <action
            android:id="@+id/action_mainFragment_to_noticeFragment"
            app:destination="@id/noticeFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
        <action
            android:id="@+id/action_mainFragment_to_searchFragment"
            app:destination="@id/searchFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
        <action
            android:id="@+id/action_mainFragment_to_detailFragment"
            app:destination="@id/detailFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
        <action
            android:id="@+id/action_mainFragment_to_searchPlaceFragment"
            app:destination="@id/searchPlaceFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
    </fragment>
    <fragment
        android:id="@+id/captureFragment"
        android:name="com.mobilescanner.main.feature_home.ui.scan.CaptureFragment"
        android:label="fragment_capture"
        tools:layout="@layout/fragment_capture" >
        <action
            android:id="@+id/action_captureFragment_to_editFragment"
            app:destination="@id/editFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
        <action
            android:id="@+id/action_captureFragment_to_processFragment"
            app:destination="@id/processFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
    </fragment>
    <fragment
        android:id="@+id/editFragment"
        android:name="com.mobilescanner.main.feature_home.ui.scan.EditFragment"
        android:label="EditFragment" >
    </fragment>
    <fragment
        android:id="@+id/processFragment"
        android:name="com.mobilescanner.main.feature_home.ui.scan.ProcessFragment"
        android:label="ProcessFragment" >
    </fragment>
    <fragment
        android:id="@+id/projectEditFragment"
        android:name="com.mobilescanner.main.feature_project.ui.ProjectEditFragment"
        android:label="fragment_project_edit"
        tools:layout="@layout/fragment_project_edit">
        <action
            android:id="@+id/action_projectEditFragment_to_captureFragment"
            app:destination="@id/captureFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/noteEditFragment"
        android:name="com.mobilescanner.main.feature_home.ui.note.NoteEditFragment"
        android:label="NoteEditFragment"
        />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.mobilescanner.main.feature_login.ui.settings.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_settingsFragment_to_userSettingsFragment"
            app:destination="@id/userSettingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/noticeFragment"
        android:name="com.mobilescanner.main.feature_home.ui.title.NoticeFragment"
        android:label="NoticeFragment" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.mobilescanner.main.feature_search.ui.SearchFragment"
        android:label="SearchFragment" />
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.mobilescanner.main.feature_community.ui.detail.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" />
    <fragment
        android:id="@+id/searchPlaceFragment"
        android:name="com.mobilescanner.main.feature_home.ui.search.SearchPlaceFragment"
        android:label="fragment_search_place"
        tools:layout="@layout/fragment_search_place"
        />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.mobilescanner.main.feature_login.ui.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"/>
        <action
            android:id="@+id/action_loginFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.mobilescanner.main.feature_login.ui.RegisterFragment"
        android:label="RegisterFragment" />
    <fragment
        android:id="@+id/userSettingsFragment"
        android:name="com.mobilescanner.main.feature_login.ui.settings.UserSettingsFragment"
        android:label="UserSettingsFragment" />
    <fragment
        android:id="@+id/preloadFragment"
        android:name="com.mobilescanner.main.feature_login.ui.PreloadFragment"
        android:label="fragment_preload"
        tools:layout="@layout/fragment_preload" >
        <action
            android:id="@+id/action_preloadFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popEnterAnim="@anim/slide_in"
            app:popExitAnim="@anim/slide_out"
            />
    </fragment>
</navigation>